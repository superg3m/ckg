{
    "c_build_version": "0.9",
    "project_name": "TESTING",
    "github_root": "https://github.com/superg3m",
    "compiler_type": "cl",
    "std_version": "c11",
    "compiler_warning_level": "",

    "compiler_disable_warnings": false,
    "debug_with_visual_studio": false,
    "should_rebuild_project_dependencies": false,
    "compiler_treat_warnings_as_errors": false,

    "project_dependencies": ["ckit"],
    "disable_specific_warnings": [""],
    "inject_as_argument": [""],

    "./build_$compiler_type": {
        "output_name": "test.exe",
        "compile_time_defines": [],
        "include_paths": [""],
        "source_files": [
            "../source/*.c"
        ],
        "additional_libs": ["../ckit/ckit_build_$compiler_type/ckit.lib"]
    },

    "execute": ["test.exe"]
}
