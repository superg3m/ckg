{
    "c_build_version": "0.9",
    "project_name": "ckg",
    "github_root": "https://github.com/superg3m",
    "compiler_type": "cl",
    "std_version": "c11",
    "compiler_warning_level": "3",

    "compiler_disable_warnings": false,
    "debug_with_visual_studio": false,
    "should_rebuild_project_dependencies": false,
    "compiler_treat_warnings_as_errors": true,

    "project_dependencies": [""],
    "disable_specific_warnings": ["4668", "4820", "4296", "4996"],
    "inject_as_argument": [""],

    "./build_$compiler_type": {
        "output_name": "ckg.lib",
        "compile_time_defines": [""],
        "include_paths": ["../core"],
        "source_files": [
            "../ckg.c"
        ],
        "additional_libs": [""]
    },

    "./example/$compiler_type" : {
        "output_name" : "test_ckg.exe",
        "compile_time_defines" : [""],
        "include_paths" : [""],
        "source_files" : [
            "../*.c"
        ],

        "additional_libs" : [
            "../../build_$compiler_type/ckg.lib"
        ]
    },

    "execute": ["test_ckg.exe"]
}