{
    "c_build_version": "0.9",
    "project_name": "ckg",
    "github_root": "https://github.com/superg3m",
    "compiler_type": "cl",
    "std_version": "c11",
    "compiler_warning_level": "all",

    "compiler_disable_warnings": false,
    "debug_with_visual_studio": false,
    "should_rebuild_project_dependencies": false,
    "compiler_treat_warnings_as_errors": true,

    "project_dependencies": [""],
    "disable_specific_warnings": [""],
    "inject_as_argument": [""],

    "./build_cl": {
        "output_name": "ckg.lib",
        "compile_time_defines": [],
        "include_paths": [""],
        "source_files": [
            "../core/Assert/*.c",
            "../core/Memory/*.c",
            "../core/String/*.c",
            "../core/Logger/*.c",
            "../core/Vector/*.c",
            "../core/LinkedList/*.c"
        ],
        "additional_libs": [""]
    },

    "./example/cl" : {
        "output_name" : "test_ckg.exe",
        "compile_time_defines" : [""],
        "include_paths" : [""],
        "source_files" : [
            "../*.c"
        ],

        "additional_libs" : [
            "../../build_cl/ckg.lib"
        ]
    },

    "execute": ["test_ckg.exe"]
}
